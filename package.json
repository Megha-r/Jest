    "antd": "^3.6.6",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.1.1",
    "enzyme-to-json": "^3.3.4",
    "fixed-data-table": "^0.6.5",
    "react": "^16.4.1",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "0.9.x",
    "reactstrap": "^6.2.0",
    "redux-logger": "^3.0.6",
